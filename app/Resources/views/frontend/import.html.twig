{% extends 'layout.html.twig' %}

{% block topMenu %}
    {% include 'partials/user_top_menu.html.twig' %}
{% endblock %}

{% block content %}
    <div class="form">
        <h4>{{ 'imdb.movie.import' | trans }}</h4>
        {{ form_start(movieForm, {'attr': {'class': 'form-tag'} }) }}
        {{ form_widget(movieForm) }}
        {{ form_end(movieForm) }}
    </div>
    <div class="form">
        <h4>{{ 'imdb.person.import' | trans }}</h4>
        {{ form_start(personForm, {'attr': {'class': 'form-tag'} }) }}
        {{ form_widget(personForm) }}
        {{ form_end(personForm) }}
    </div>
{% endblock %}
